@model IEnumerable<Inscripcion_Universidad.Models.Dominio.MateriaPorCorrelativa>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Materia.NombreMateria)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrimeraCorrelativa.NombreCorrelativa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SegundaCorrelativa.NombreCorrelativa)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Materia.NombreMateria)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrimeraCorrelativa.NombreCorrelativa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SegundaCorrelativa.NombreCorrelativa)
        </td>
        <td>
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
